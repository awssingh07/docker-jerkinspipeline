pipeline {
  environment {
    registry = “saran07/docker-test”
    registryCredential = ‘dockerhub’
    dockerImage = ‘’
  }
  agent any
  stages {
    stage(‘Cloning Git’) {
      steps {
        git ‘https://github.com/awssingh07/docker-jerkinspipeline.git'
      }
    }
    stage(‘Building image’) {
      steps{
        script {
          docker.build registry + “:$BUILD_NUMBER”
        }
      }
    }
  }
}
